<?xml version='1.0' encoding='utf-8'?>
<widget id="lhidalgo42.nicolas.triantafilo" version="1.3.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Medicina Hospitalaria</name>
    <description>
        Gu√≠a para el residente de turno.
    </description>
    <author email="lhidalgosep@gmail.com" href="http://facebook.com/leoohidalgo">
        Leonardo Hidalgo
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="android-minSdkVersion" value="21" />
    <preference name="android-targetSdkVersion" value="23" />
    <preference name="android-maxSdkVersion" value="0" />
    <preference name="android-installLocation" value="auto" />
    <preference name="orientation" value="default" />
    <preference name="fullscreen" value="true" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="img/icon.png" />
    <engine name="android" spec="*" />
    <plugin name="cordova-plugin-media" spec="^3.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
</widget>
